{
  "name": "url-shortener",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "ng serve -c development",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "emulators:start": "(cd functions && npm run-script build) && firebase emulators:start",
    "emulators:kill": "npx kill-port 4000 4400 5000 5001 8080 8085 9099 3000",
    "test": "ng test",
    "install": "(cd functions && npm install) && npm install"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
